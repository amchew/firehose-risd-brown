
<h2>
	<!-- this is like var dump -->
	<% @pics.each do |pic|%>
	<div class="span4 text-centre helloBox">

		<h1 class="custom-font">
			Hour: <%= pic.hour %>
		</h1>
		<div class="text-center"><%= image_tag(pic.image) %></div>
		<h2>
			<br/>
			Emotion: <%= pic.emotion %>
			<br/><br/>
			Learned: <%= pic.learned %>

		</h2>

	</div>
	<% end %>
</h2>
